<table class="table table-hover">
  <thead>
    <tr>
      <th class="col-md-9">Name</th>
      <th class="col-md-3">Year</th>
    </tr>
  </thead>

  <tbody>
    <% breweries.each do |brewery| %>
      <tr>
        <td><%= link_to brewery.name, brewery %></td>
        <td><%= brewery.year %></td>
      </tr>
    <% end %>
  </tbody>
</table>